[global]
#name=
#directory=
filename=/dev/nvme0n1
#size=

ioengine=libaio

direct=1
#buffered=0
invalidate=1
#atomic=

#time_based
#runtime=

thread
#numjobs=4

group_reporting

[sequential-fill]
readwrite=write
blocksize=1M
iodepth=16

[random-fill]
readwrite=randwrite
blocksize=4K
randrepeat=0
norandommap
iodepth=32
numjobs=4
stonewall

